<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistroActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Club Deportivo"
        android:textSize="32sp"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="105dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Grupo 10"
        android:textSize="32sp"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="156dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="338dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="213dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        android:layout_width="347dp"
        android:layout_height="60dp"
        android:hint="Ingrese su nombre completo"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="209dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editText4"
        android:layout_width="347dp"
        android:layout_height="60dp"
        android:hint="Repetir contraseña"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="472dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editText3"
        android:layout_width="347dp"
        android:layout_height="60dp"
        android:hint="Ingrese su contraseña"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="404dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="338dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="279dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editText"
        android:layout_width="347dp"
        android:layout_height="60dp"
        android:hint="Ingrese su email"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="275dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="338dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="408dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editText2"
        android:layout_width="347dp"
        android:layout_height="60dp"
        android:hint="Nº telefono sin 0 ni 15"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="335dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="338dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="476dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="338dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="339dp">

    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/btnRegistrarse"
        android:layout_width="237dp"
        android:layout_height="57dp"
        android:backgroundTint="#7480FF"
        android:fontFamily="sans-serif"
        android:text="Registrarse"
        android:textSize="16sp"
        android:typeface="normal"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="557dp" />

    <TextView
        android:id="@+id/txtRegistro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="Cancelar"
        android:textSize="14sp"
        android:typeface="normal"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="635dp" />
</androidx.constraintlayout.widget.ConstraintLayout>